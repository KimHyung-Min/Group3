<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE configuration
                PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
                "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="false" />
        <setting name="jdbcTypeForNull" value="NULL" />
        <setting name="callSettersOnNulls" value="true"/>
    </settings>
    <!-- Alias 설정 -->
    <typeAliases>
        <typeAlias alias="member" type="model.vo.MemberVO" />
        <typeAlias alias="nonmember" type="model.vo.NonMemberVO" />
        <typeAlias alias="book" type="model.vo.BookVO"/>
        <typeAlias alias="publisher" type="model.vo.PublisherVO" />
        <typeAlias alias="cart" type="model.vo.ShoppingCartVO" />
        <typeAlias alias="purchase" type="model.vo.PurchaseVO" />
        <typeAlias alias="salesdata" type="model.vo.SalesDataVO"/>
        <typeAlias alias="purchaseBook" type="model.vo.PurchaseBookVO" />
        <typeAlias alias="review" type="model.vo.ReviewVO"/>
        <typeAlias alias="customerBoard" type="model.vo.CustomerBoardVO" />
    </typeAliases>

</configuration>